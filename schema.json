      "csid-2": {
        "disabled": false,
        "hidden": false,
        "value": {
          "resolverName": "formula",
          "resolverData": "CRAWL(subInputs, '${key: value}')"
        },
        "validators": [],
        "dirty": {
          "resolverName": "formula",
          "resolverData": "SOME(CRAWL(subInputs, '[dirty]'))"
        },
        "touched": {
          "resolverName": "formula",
          "resolverData": "SOME(CRAWL(subInputs, '[touched]'))"
        },
        "validity": {
          "initial": {
            "valid": true,
            "errors": []
          },
          "resolverName": "formula",
          "resolverData": "OBJECT('valid', AND(CRAWL(selfValidity, 'valid'), subInputsValid), 'errors', CRAWL(selfValidity, 'errors'))"
        },
        "subInputs": {
          "resolverName": "formula",
          "resolverData": "COLLECT('children :input')"
        },
        "selfValidity": {
          "initial": {
            "valid": true,
            "errors": []
          },
          "resolverName": "formula",
          "resolverData": "VALIDATE(value, validators)"
        },
        "subInputsValid": {
          "resolverName": "formula",
          "resolverData": "EVERY(CRAWL(subInputs, '[validity.valid]'))"
        },
        "children": [
          "csid-3",
          "csid-4",
          "csid-6",
          "csid-8",
          "csid-10"
        ],
        "keys": {
          "resolverName": "formula",
          "resolverData": "CRAWL(subInputs, '$append([], [key])')"
        },
        "id": "csid-2",
        "customId": "form",
        "name": "inputGroup",
        "parentId": "csid-1",
        "contextVariables": {},
        "hooks": [
          {
            "trigger": {
              "name": "afterValueChange"
            },
            "runner": {
              "name": "js",
              "data": "\n          const value = await getProperty('', 'value') || {};\n          const keys = await getProperty('', 'keys');\n\n          for (const key of keys) {\n            await setValue('[key=' + key + ']', value[key]);\n          }\n        "
            },
            "overridable": false
          },
          {
            "trigger": {
              "name": "afterComponentRender"
            },
            "runner": {
              "name": "js",
              "data": "const value = await http({ method: 'get', url: 'users' });\nawait setProperty('', 'value', value);\n            "
            }
          }
        ]
